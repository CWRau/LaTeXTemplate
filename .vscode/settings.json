// Place your settings in this file to overwrite default and user settings.
{
  "files.exclude": {
    "**/.git": true,
    "**/.svn": true,
    "**/.hg": true,
    "**/.DS_Store": true,
    "**/*.aux": true,
    "**/*.acn": true,
    "**/*.bbl": true,
    "**/*.bcf": true,
    "**/*.blg": true,
    "**/*.fdb_latexmk": true,
    "**/*.fk": true,
    "**/*.fls": true,
    "**/*.ist": true,
    "**/*.lof": true,
    "**/*.lol": true,
    "**/*.maf": true,
    "**/*.mtc": true,
    "**/*.mtc0": true,
    "**/*.out": true,
    "**/*.run.xml": true,
    "**/*.synctex.gz": true,
    "**/*.synctex.gz(busy)": true,
    "**/*.toc": true,
    "**/*.log": true,
    "**/*.lot": true,
    "**/*.acr": true,
    "**/*.alg": true,
    "**/*.xwm": true,
    "**/*.swp": true,
    "**/.gitmodules": false,
    "**/*.glsdefs": true,
    "**/~$*": true,
    "**/.vscode": true
  },
  "latex-workshop.latex.autoBuild.onSave.enabled": false,
  "latex-workshop.latex.clean.enabled": true,
  "cSpell.enabled": false,
  "editor.insertSpaces": false,
  "editor.detectIndentation": true
}
